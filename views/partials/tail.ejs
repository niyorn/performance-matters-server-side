
<script>
    if('serviceWorker' in navigator){
        window.addEventListener('load', function () {
            navigator.serviceWorker.register('/script/sw.js')
            .then(function (registration) {
                console.log(`service worker succesfully installed: ${registration.scope}`)
            })
            .catch(function (err) {
                console.log(`service worker failed installing: ${err}`)
            })
        })
    }
</script>
</body>
</html> 